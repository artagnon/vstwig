{
  "comments": {
    "blockComment": [
      ["{#", "#}"],
      ["<!--", "-->"]
    ]
  },
  "brackets": [
    ["{{", "}}"],
    ["{%", "%}"]
  ],
  "autoClosingPairs": [
    ["{{", "}}"],
    ["{%", "%}"]
  ],
  "surroundingPairs": [
    ["{{", "}}"],
    ["<", ">"],
    ["{", "}"],
    ["\"", "\""],
    ["{%", "%}"]
  ],
  "indentationRules": {
    "increaseIndentPattern": "(<\\w+>|{% [^(end)] %})",
    "decreaseIndentPattern": "(</\\w+>|{% end\\w+ %})"
  }
}
